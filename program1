//Documentation section
#include<stdio.h>
                               //main function
void main()
{
        
    int a, b, c,large;

    printf("Enter a: ");              //Gathering data
    scanf("%d", &a);

    printf("Enter b: ");
    scanf("%d", &b);

    printf("Enter c: ");
    scanf("%d", &c);
     large= a>b?(a>c?a:c):(b>c?b:c);             //ternary operator used
     printf("The largest of three numbers is",large);
 
 return 0;                  //return function
}

    

